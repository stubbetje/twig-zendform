  "description": "Twig extension to integrate zendframework/zend-form into twig templates",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Stefan Stubbe",
      "email": "stefan.stubbe@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "twig/twig": "^1.24",
    "zendframework/zend-form": "^2.9",
    "zendframework/zend-escaper": "^2.5"
  },
  "require-dev": {
    "zendframework/zend-servicemanager": "^3.1",
    "phpunit/phpunit": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "Stubbetje\\Twig\\ZendForm\\": "src/"
    }
  }
}
